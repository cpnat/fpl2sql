{
  "name": "fpl2sql-frontend",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:html": "prettier --write '**/*.html'",
    "format:ts": "prettier --write '**/*.{ts,tsx}'",
